{
  "name": "ydm-antd",
  "version": "0.0.14",
  "private": false,
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "build/index.umd.js",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "classnames": "^2.3.2",
    "react-transition-group": "^4.4.5"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "scripts": {
    "lint": "eslint --ext js,ts,tsx components",
    "start": "webpack serve",
    "test": "npx jest",
    "build": "npm run clean && cross-env NODE_ENV=production NODE_OPTIONS=\"--experimental-json-modules\" gulp && npm run build-umd && npm run build-css",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-css": "node-sass ./components/styles/index.scss ./build/index.css",
    "prepare": "husky install",
    "build-es": "rollup --config rollup/rollup.esm.config.js",
    "build-umd": "rollup --config rollup/rollup.umd.config.js",
    "clean:lib": "rimraf lib",
    "clean:es": "rimraf es",
    "clean:build": "rimraf build",
    "cpublish": "node ./bin/dev.js && npm run build && npm publish",
    "clean": "npm run clean:lib && npm run clean:es &&  npm run clean:build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run lint"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.0",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^7.4.0",
    "@storybook/react-webpack5": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^29.5.5",
    "@types/react-transition-group": "^4.4.6",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-named-exports-order": "^0.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint-plugin-storybook": "^0.6.13",
    "gulp": "^4.0.2",
    "gulp-dart-sass": "^1.1.0",
    "gulp-filter": "^8.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "jest-environment-jsdom": "^29.7.0",
    "merge2": "^1.4.1",
    "node-sass": "^7.0.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "rimraf": "^5.0.5",
    "rollup": "^2.79.1",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.23",
    "rollup-plugin-sass": "^1.12.20",
    "rollup-plugin-typescript2": "^0.35.0",
    "sass-loader": "^13.3.2",
    "storybook": "^7.4.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "web-vitals": "^2.1.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
